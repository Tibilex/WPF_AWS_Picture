<Window x:Class="AWS_Picture.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AWS_Picture"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000">

    <Grid Background="#2c2e32">
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Border Grid.Row="0" BorderBrush="#3c3e42" BorderThickness="0 0 0 1">
            <Grid  Background="#272727">

            </Grid>
        </Border>
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="5cm"/>
            </Grid.ColumnDefinitions>
            <Border Grid.Column="0" BorderBrush="#3c3e42" BorderThickness="0 0 1 0">
                <Grid>

                </Grid>
            </Border>
            <Grid Grid.Column="1">

            </Grid>
        </Grid>
    </Grid>
</Window>
